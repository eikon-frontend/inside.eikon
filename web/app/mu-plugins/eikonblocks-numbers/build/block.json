{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "eikonblocks/numbers",
  "version": "0.1.0",
  "title": "Numbers block",
  "category": "widgets",
  "icon": "format-quote",
  "description": "Number block scaffolded with Create Block tool.",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "eikonblocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "items": {
      "type": "array",
      "default": []
    },
    "backgroundColor": {
      "type": "string",
      "default": "fuchsia"
    },
    "textColor": {
      "type": "string",
      "default": "blue"
    }
  }
}