{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "eikonblocks/card",
  "version": "0.1.0",
  "title": "Card",
  "category": "common",
  "description": "Card block with an image and inner blocks.",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "eikonblocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "parent": ["eikonblocks/section"],
  "allowedBlocks": [
    "eikonblocks/heading",
    "eikonblocks/buttons",
    "core/image",
    "core/paragraph",
    "core/list",
    "core/list-item"
  ],
  "attributes": {
    "imageUrl": {
      "type": "string",
      "default": ""
    },
    "imagePosition": {
      "type": "string",
      "default": "right"
    }
  }
}
