{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "eikonblocks/marquee",
  "version": "0.1.0",
  "title": "Marquee",
  "category": "widgets",
  "icon": "editor-textcolor",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "eikonblocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": "p.content"
    },
    "backgroundColor": {
      "type": "string",
      "default": "white"
    },
    "textColor": {
      "type": "string",
      "default": "blue"
    }
  }
}
