{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "eikonblocks/section",
  "version": "0.1.0",
  "title": "Section",
  "category": "common",
  "icon": "section",
  "description": "Section block scaffolded with Create Block tool.",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "eikonblocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "allowedBlocks": [
    "eikonblocks/accordion",
    "eikonblocks/projects",
    "eikonblocks/departments-teaser",
    "eikonblocks/heading",
    "eikonblocks/marquee",
    "eikonblocks/numbers",
    "eikonblocks/buttons",
    "eikonblocks/mixedposts",
    "eikonblocks/card",
    "eikonblocks/grid",
    "core/image",
    "core/paragraph",
    "core/list",
    "core/list-item"
  ],
  "attributes": {
    "backgroundColor": {
      "type": "string",
      "default": "white"
    },
    "textColor": {
      "type": "string",
      "default": "blue"
    },
    "paddingTop": {
      "type": "string",
      "default": "big"
    },
    "paddingBottom": {
      "type": "string",
      "default": "big"
    },
    "isPaddingSymmetrical": {
      "type": "boolean",
      "default": true
    }
  }
}